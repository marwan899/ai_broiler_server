{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    { 
      "src": "/admin-dashboard.html", 
      "dest": "/admin-dashboard.html" 
    },
    { 
      "src": "/index.html", 
      "dest": "/index.html" 
    },
    { 
      "src": "/",
      "dest": "/index.html" 
    }
  ]
}
