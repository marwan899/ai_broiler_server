{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    { 
      "src": "/admin-dashboard.html", 
      "dest": "/admin-dashboard.html" 
    },
    { 
      "src": "/(.*)",
      "dest": "/index (1).html" 
    }
  ],
  "rewrites": [
    { 
      "source": "/api/index/(.*)", 
      "destination": "/api/index.js" 
    }
  ]
}
