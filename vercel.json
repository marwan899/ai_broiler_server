{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { 
      "source": "/api/index/records/(.*)",
      "destination": "/api/index.js" 
    },
    { 
      "source": "/api/index/flock/(.*)",
      "destination": "/api/index.js" 
    },
    { 
      "source": "/admin-dashboard.html", 
      "destination": "/admin-dashboard.html" 
    },
    { 
      "source": "/(.*)",
      "destination": "/index (1).html" 
    }
  ]
}
