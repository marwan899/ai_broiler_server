{
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { 
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    { 
      "source": "/admin-dashboard.html", 
      "destination": "/admin-dashboard.html" 
    },
    { 
      "source": "/index.html", 
      "destination": "/index.html" 
    },
    { 
      "source": "/",
      "destination": "/index.html" 
    }
  ]
}
